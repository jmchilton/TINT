<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Patrick Pedrioli (Institute For Systems Biology) --><xs:schema targetNamespace="http://sashimi.sourceforge.net/schema_revision/mzXML_3.0" xmlns:csp="http://sashimi.sourceforge.net/schema/separation/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cff="http://sashimi.sourceforge.net/schema_revision/mzXML_3.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">	<xs:include schemaLocation="separation_technique_1.0.xsd"/>	<xs:include schemaLocation="general_types_1.0.xsd"/>	<xs:element name="column" type="cff:columnType" substitutionGroup="cff:separationTechnique">		<xs:annotation>			<xs:documentation>This is an example implementation for a separation element to substitute into the mzXML schema.</xs:documentation>		</xs:annotation>	</xs:element>	<xs:complexType name="columnType">		<xs:complexContent>			<xs:extension base="cff:separationTechniqueType">				<xs:sequence>					<xs:element name="event" maxOccurs="unbounded">						<xs:annotation>							<xs:documentation>Each event element describe a change in the separation condition at a given time</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="time" type="xs:duration">									<xs:annotation>										<xs:documentation>Time of the change</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="flow">									<xs:annotation>										<xs:documentation>Flow rate at the time of the change.</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:simpleContent>											<xs:extension base="xs:float">												<xs:attribute name="unit" type="xs:string" use="required">													<xs:annotation>														<xs:documentation>Flow rate units</xs:documentation>													</xs:annotation>												</xs:attribute>											</xs:extension>										</xs:simpleContent>									</xs:complexType>								</xs:element>								<xs:element name="percentageB" type="xs:float" minOccurs="0">									<xs:annotation>										<xs:documentation>Percentage of solvent B (LC)</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="celsiusDegree" minOccurs="0">									<xs:annotation>										<xs:documentation>Temperature in Celsius</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:simpleContent>											<xs:extension base="xs:float"/>										</xs:simpleContent>									</xs:complexType>								</xs:element>								<xs:element name="nameValue" type="cff:namevalueType" minOccurs="0" maxOccurs="unbounded">									<xs:annotation>										<xs:documentation>This is reserved for describing separation condition not elsewhere included in the schema</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType></xs:schema>
